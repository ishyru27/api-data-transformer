{
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": true,
    "overrides": [
        {
            "files": ["*.json", "*.md", "*.toml", "*.yml"],
            "options": {
                "useTabs": false
            }
        },
        {
            "files": "*.ts,*.tsx,*.js,*.jsx"
        }
    ],
    "endOfLine": "lf"
}
